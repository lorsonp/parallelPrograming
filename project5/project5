#!/bin/bash


for k in {1024,2048,4096,8192,16384,32768,65536,131072,262144,524288,1048576,2097152,4194304,8388608}
do
for i in {32,64,128,256}
do
  echo $k
  g++ -DNUM_ELEMENTS=$k -DLOCAL_SIZE=$i -o first first.cpp /scratch/Paige/parallelPrograming/project5/libOpenCL.so -lm -fopenmp
  ./first
done
done
