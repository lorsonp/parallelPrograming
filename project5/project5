#!/bin/bash


for k in {1000,2500,5000,7500,10000,25000,50000,75000,100000,250000,500000,750000,1000000,2500000,5000000,7500000,10000000}
do
for i in {8	16 32	64 128 256 512}
do

  echo $k

  g++ -DNUM_ELEMENTS=$k -DLOCAL_SIZE=$i -o first first.cpp /scratch/Paige/parallelPrograming/project5/libOpenCL.so -lm -fopenmp

done
done
